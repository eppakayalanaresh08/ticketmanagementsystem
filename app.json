{
  "expo": {
    "name": "CSM",
    "slug": "CSM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TadcmsLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/TadcmsLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f3037"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "url": "https://u.expo.dev/d6f63307-dc37-4c1f-b68c-6b05f2698c10"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nareshweb.myapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/TadcmsLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nareshweb.myapp",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "platforms": [
      "ios",
      "android"
    ],
    "permissions": {
      "location": {
        "foreground": true,
        "always": true
      }
    },
    "web": {
      "favicon": "./assets/TadcmsLogo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/TadcmsLogo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/ticketsound.wav"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "d6f63307-dc37-4c1f-b68c-6b05f2698c10"
      }
    }
  }
}
